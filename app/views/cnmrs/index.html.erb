<h1>Listing cnmrs</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Filename</th>
      <th>Partanalysis</th>
      <th>Solvent1</th>
      <th>Solvent2</th>
      <th>Sumc</th>
      <th>Instrument</th>
      <th>Method</th>
      <th>Pulseprogram</th>
      <th>Temp</th>
      <th>Scans</th>
      <th>Buffera</th>
      <th>Oka</th>
      <th>Okb</th>
      <th>Okc</th>
      <th>Okd</th>
      <th>Singlepart</th>
      <th>Description</th>
      <th>Fulldoc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cnmrs.each do |cnmr| %>
      <tr>
        <td><%= cnmr.name %></td>
        <td><%= cnmr.filename %></td>
        <td><%= cnmr.partanalysis %></td>
        <td><%= cnmr.solvent1 %></td>
        <td><%= cnmr.solvent2 %></td>
        <td><%= cnmr.sumc %></td>
        <td><%= cnmr.instrument %></td>
        <td><%= cnmr.method %></td>
        <td><%= cnmr.pulseprogram %></td>
        <td><%= cnmr.temp %></td>
        <td><%= cnmr.scans %></td>
        <td><%= cnmr.buffera %></td>
        <td><%= cnmr.oka %></td>
        <td><%= cnmr.okb %></td>
        <td><%= cnmr.okc %></td>
        <td><%= cnmr.okd %></td>
        <td><%= cnmr.singlepart_id %></td>
        <td><%= cnmr.description_id %></td>
        <td><%= cnmr.fulldoc_id %></td>
        <td><%= link_to 'Show', cnmr %></td>
        <td><%= link_to 'Edit', edit_cnmr_path(cnmr) %></td>
        <td><%= link_to 'Destroy', cnmr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cnmr', new_cnmr_path %>
