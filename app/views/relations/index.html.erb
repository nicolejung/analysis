<h1>Listing relations</h1>

<table class="table">
  <thead>
    <tr>
      <th>Multiplet</th>
      <th>Givej</th>
      <th>Distance</th>
      <th>Detail</th>
      <th>Fill1</th>
      <th>Hz</th>
      <th>Oka</th>
      <th>Okb</th>
      <th>Fulldoc</th>
      <th>Singlepart</th>
      <th>Description</th>
      <th>Hnmr</th>
      <th>Nmrdetailh</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relations.each do |relation| %>
      <tr>
        <td><%= relation.multiplet %></td>
        <td><%= relation.givej %></td>
        <td><%= relation.distance %></td>
        <td><%= relation.detail %></td>
        <td><%= relation.fill1 %></td>
        <td><%= relation.hz %></td>
        <td><%= relation.oka %></td>
        <td><%= relation.okb %></td>
        <td><%= relation.fulldoc_id %></td>
        <td><%= relation.singlepart_id %></td>
        <td><%= relation.description_id %></td>
        <td><%= relation.hnmr_id %></td>
        <td><%= relation.nmrdetailh_id %></td>
        <td><%= link_to 'Show', relation %></td>
        <td><%= link_to 'Edit', edit_relation_path(relation) %></td>
        <td><%= link_to 'Destroy', relation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relation', new_relation_path %>
